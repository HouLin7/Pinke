<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="background_tab_pressed">#6633B5E5</color>
    <color name="black">#000000</color>
    <color name="red">#FF3737</color>
    <color name="text_black">#ff333333</color>
    <color name="white">#fff9f9f9</color>
    <dimen name="divier_height">1dp</dimen>
    <string name="app_name">AppLibrary</string>
    <string name="cancel">取消</string>
    <string name="company_copyright_name">国美电器有限公司</string>
    <string name="confirm">确认</string>
    <string name="down_error">下载失败</string>
    <string name="down_ing">美办下载中，请稍后...</string>
    <string name="down_ok">美办下载成功，点击安装</string>
    <string name="hello_world">Hello world!</string>
    <string name="loading_hint">正在加载中，请稍后…</string>
    <string name="network_error">网络错误</string>
    <string name="no_more_new_version">您当前已是最新版本</string>
    <string name="owner">持有人:</string>
    <style name="AppBaseTheme" parent="Theme.AppCompat.Light.NoActionBar">

        
        <item name="colorPrimaryDark">@color/theme_blue</item>
        
        <item name="colorPrimary">@color/theme_blue</item>
        <item name="colorAccent">@color/theme_blue_gray</item>
        
        <item name="drawerArrowStyle">@style/DrawerArrowToggle</item>
        <item name="android:actionMenuTextColor">@color/white</item>
        <item name="colorControlNormal">@color/white</item>
        <item name="android:actionMenuTextAppearance">@style/MenuTextStyle</item>
    </style>
    <style name="DrawerArrowToggle" parent="Widget.AppCompat.DrawerArrowToggle">
        <item name="color">@color/white</item>
    </style>
    <style name="MenuTextStyle">
        <item name="android:textSize">16dp</item>
    </style>
    <style name="pop_window_animation_fade" parent="android:Animation">
            <item name="android:windowEnterAnimation">@anim/fade_in</item>
            <item name="android:windowExitAnimation">@anim/fade_out</item>
        </style>
    <declare-styleable name="CircleIndicator">
        <attr format="dimension" name="ci_width"/>
        <attr format="dimension" name="ci_height"/>
        <attr format="dimension" name="ci_margin"/>
        <attr format="reference" name="ci_animator"/>
        <attr format="reference" name="ci_animator_reverse"/>
        <attr format="reference" name="ci_drawable"/>
        <attr format="reference" name="ci_drawable_unselected"/>
    </declare-styleable>
    <declare-styleable name="PagerSlidingTabStrip">
        <attr format="color" name="pstsIndicatorColor"/>
        <attr format="color" name="pstsUnderlineColor"/>
        <attr format="color" name="pstsDividerColor"/>
        <attr format="dimension" name="pstsIndicatorHeight"/>
        <attr format="dimension" name="pstsUnderlineHeight"/>
        <attr format="dimension" name="pstsDividerPadding"/>
        <attr format="dimension" name="pstsDividerPaddingTopBottom"/>
        <attr format="dimension" name="pstsTabPaddingLeftRight"/>
        <attr format="dimension" name="pstsScrollOffset"/>
        <attr format="reference" name="pstsTabBackground"/>
        
        <attr format="boolean" name="pstsShouldExpand"/>
        <attr format="boolean" name="pstsTextAllCaps"/>
        <attr format="color" name="pstsTextSelectedColor"/>
        
        <attr format="float" name="pstsScaleZoomMax"/>
        <attr format="boolean" name="pstsSmoothScrollWhenClickTab"/>
        
        
    </declare-styleable>
    <declare-styleable name="RoundProgressBar">
        <attr format="color" name="roundColor"/>
        <attr format="color" name="roundProgressColor"/>
        <attr format="dimension" name="roundWidth"/>
        <attr format="color" name="textColor"/>
        <attr format="dimension" name="textSize"/>
        <attr format="integer" name="max"/>
        <attr format="boolean" name="textIsDisplayable"/>
        <attr name="style">
            <enum name="STROKE" value="0"/>
            <enum name="FILL" value="1"/>
        </attr>
    </declare-styleable>
</resources>